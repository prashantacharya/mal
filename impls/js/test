(
  def! areaOfCircle (
    fn* (r) (* 3.14 (^ r 2))
  )
)

(
  def! perimeter (
    fn* (l b) ( * 2 (+ l b) )
  )
)

(
  def! isEven (
    fn* (num) (= (% num 2) 0)
  )
)

(def! fib (
  fn* (N) (
      if (<= N 2) 1 (+ (fib (- N 1)) (fib (- N 2)))
    )
  )
)

; Create a list
(def! l (list 1 2 3))

; Check if list
(list? (list 1 2 3))

; Check if list is empty
(empty? (list))

; Count list elements
(count (list 1 2 3 4 5 6))
